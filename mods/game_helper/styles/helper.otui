HelperRules < NewHeadlessWindow
  id: helperRules
  anchors.horizontalCenter: parent.horizontalCenter
  size: 450 345
  @onEscape: hide()
  UIWidget
    size: 145 56
    anchors.top: parent.top
    anchors.horizontalCenter: parent.horizontalCenter
    margin-top: -33
    image-source: /images/game/tutorial/backdrop_crest
    phantom: true

  Panel
    id: contentPanel
    anchors.fill: parent
    margin: 15 13 17 13
    TextList
      id: rules
      anchors.top: parent.top
      anchors.left: parent.left
      anchors.right: parent.right
      anchors.bottom: separator.top
      margin-top: 10
      margin-bottom: 10
      text-align: center
      padding: 1
      focusable: false
      background-color: #484848
    
    HorizontalSeparator
      id: separator
      anchors.bottom: next.top
      anchors.left: parent.left
      anchors.right: parent.right
      margin-bottom: 10
    Button
      id: next
      anchors.bottom: parent.bottom
      anchors.right: parent.right
      !text: tr('Next')
      size: 43 20
      enabled: false
      @onClick: modules.game_helper:onHelperTermConditionNext()
    Button
      id: close
      anchors.bottom: parent.bottom
      anchors.right: prev.left
      margin-right: 5
      !text: tr('Close')
      size: 43 20
      @onClick: modules.game_helper.closeTerms()
    CheckBox
      id: termCondition
      width: 100
      anchors.bottom: parent.bottom
      anchors.left: parent.left
      text-offset: 16 -2
      !text: tr('I agree with RubinOT Terms and Conditions.')
      !tooltip: tr('I have read and I agree to the Extended RubinOT Service Agreement and the RubinOT Privacy Policy.')
      margin-bottom: 5
      font: Verdana Bold-11px
      color: #c0c0c0
      text-auto-resize: true
      @onCheckChange: modules.game_helper.onHelperTermCondition(self:getId(), self:isChecked())

PlayerName < Label
