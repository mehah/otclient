HelperTracker < MiniWindow
  id: helperMiniWindow
  !text: tr('Helper Stats')
  icon: /images/icons/icon-healing
  height: 145
  &type: 'helper'
  MiniWindowContents
    id: contentsPanel
    anchors.fill: parent
    margin: 20 9 5 9
    Label
      id: helperLabel
      anchors.top: parent.top
      anchors.left: parent.left
      !text: tr('General Helper')
      font: Verdana Bold-11px
      color: #c0c0c0
      text-auto-resize: true
    UIWidget
      id: hierarchy
      size: 7 7
      anchors.top: prev.bottom
      anchors.left: parent.left
      margin-top: 5
      margin-left: 5
      image-source: /images/ui/hierarchy-branch
      phantom: true
    UIWidget
      id: helperStatus
      size: 155 15
      anchors.top: prev.top
      anchors.left: prev.right
      !text: tr('Inactive')
      margin-top: -3
      margin-left: 5
      font: Verdana Bold-11px
      color: #D33C3C
      text-auto-resize: true
      tooltip-delayed: true

    Label
      id: targetLabel
      anchors.top: prev.bottom
      anchors.left: parent.left
      margin-top: 3
      !text: tr('Target Helper')
      font: Verdana Bold-11px
      color: #c0c0c0
      text-auto-resize: true
    UIWidget
      id: hierarchy
      size: 7 7
      anchors.top: prev.bottom
      anchors.left: parent.left
      margin-top: 5
      margin-left: 5
      image-source: /images/ui/hierarchy-branch
      phantom: true
    UIWidget
      id: targetStatus
      size: 155 15
      anchors.top: prev.top
      anchors.left: prev.right
      !text: tr('Inactive')
      margin-top: -3
      margin-left: 5
      font: Verdana Bold-11px
      color: #D33C3C
      text-auto-resize: true
      tooltip-delayed: true

    Label
      id: shooterLabel
      anchors.top: prev.bottom
      anchors.left: parent.left
      margin-top: 3
      !text: tr('Shooter Helper')
      font: Verdana Bold-11px
      color: #c0c0c0
      text-auto-resize: true
    UIWidget
      id: hierarchy
      size: 7 7
      anchors.top: prev.bottom
      anchors.left: parent.left
      margin-top: 5
      margin-left: 5
      image-source: /images/ui/hierarchy-branch
      phantom: true
    UIWidget
      id: shooterStatus
      size: 155 15
      anchors.top: prev.top
      anchors.left: prev.right
      !text: tr('Inactive')
