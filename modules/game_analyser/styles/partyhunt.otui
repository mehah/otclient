LeaderInfo < UIWidget
  size: 151 63
  margin-top: 5
  UIWidget
    id: leaderPt
    size: 11 11
    image-source: /images/game/shields/shields
    image-clip: 0 0 11 11
    anchors.top: parent.top
    anchors.left: parent.left
  Label
    id: name
    !text: tr('Name')
    anchors.top: prev.top
    anchors.left: prev.right
    text-auto-resize: true
    color: #ffda34
    font: Verdana Bold-11px
    margin-left: 3
    margin-top: -3
  UIWidget
    id: balanceIcon
    size: 11 11
    image-auto-resize: true
    image-source: /images/icons/icon-balance
    anchors.top: prev.bottom
    anchors.left: parent.left
    margin-top: 5
    margin-left: 7
    !tooltip: tr('Balance')
  UIWidget
    id: damageIcon
    size: 11 11
    image-auto-resize: true
    image-source: /images/icons/icon-damage
    anchors.top: prev.bottom
    anchors.left: parent.left
    margin-top: 8
    margin-left: 7
    !tooltip: tr('Damage')
  UIWidget
    id: healingIcon
    size: 11 11
    image-auto-resize: true
    image-source: /images/icons/icon-healing
    anchors.top: prev.bottom
    anchors.left: parent.left
    margin-top: 8
    margin-left: 7
    !tooltip: tr('Healing')
  UIWidget
    id: gold
    size: 9 9
    image-auto-resize: true
    image-source: /game_cyclopedia/images/icon_gold
    anchors.top: balanceIcon.top
    anchors.right: parent.right
    margin-top: 2
    margin-right: 3
  UIWidget
    id: balance
    !text: tr('0')
    anchors.top: balanceIcon.top
    anchors.right: prev.left
    margin-top: -2
    margin-right: 4
    text-auto-resize: true
    color: #44ad25
    font: Verdana Bold-11px
    tooltip-font: Verdana Bold-11px-wheel
    tooltip-delayed: true
  Label
    id: damage
    !text: tr('0')
    anchors.top: damageIcon.top
    anchors.right: parent.right
    margin-top: -2
    margin-right: 2
    text-auto-resize: true
    color: #c0c0c0
    font: Verdana Bold-11px
  Label
    id: healing
    !text: tr('0')
    anchors.top: healingIcon.top
    anchors.right: parent.right
    margin-top: -2
    margin-right: 2
