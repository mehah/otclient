DeathWindow < MainWindow
  id: DeathWindow
  !text: tr('You are dead')
  &baseWidth: 350
  &baseHeight: 15
  min-width: 350
  max-width: 600

  Label
    id: labelText
    width: 550
    height: 140
    anchors.left: parent.left
    anchors.top: parent.top
    margin-left: 10
    margin-top: 2

  Button
    id: buttonOk
    !text: tr('Respawn')
    width: 80
    anchors.bottom: parent.bottom
    anchors.horizontalCenter: parent.horizontalCenter
