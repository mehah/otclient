    anchors.top: prev.bottom
    margin-top: 6
    image-source: /images/ui/checkbox
    image-clip: 0 0 12 12
    change-cursor-image: false

    $hover !disabled:
      color: #c0c0c0

  Label
    anchors.top: prev.bottom
    anchors.left: parent.left
    margin-top: 10
    !text: tr('Items:')
    text-auto-resize: true
    font: Verdana Bold-11px
    color: #C0C0C0

